{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "Faq"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "question",
      "required": true
    },
    "answer": {
      "type": "blocks",
      "required": true
    },
    "order": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "isActive": {
      "type": "boolean",
      "required": false,
      "default": true
    }
  }
}
